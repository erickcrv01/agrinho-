// Adicionar evento de click nos botões de comprar
document.querySelectorAll('button').forEach(button => {
    button.addEventListener('click', () => {
        // Adicionar produto ao carrinho
        const produto = button.parentNode;
        const carrinho = document.querySelector('.carrinho');
        const clone = produto.cloneNode(true);
        carrinho.appendChild(clone);
        // Atualizar total do carrinho
        const total = document.querySelector('.total');
        const preco = produto.querySelector('p').textContent;
        total.textContent = parseFloat(total.textContent) + parseFloat(preco);
    });
});

// Adicionar evento de click no botão de finalizar compra
